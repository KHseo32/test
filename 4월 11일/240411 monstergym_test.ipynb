       "<script>\n",
       "        // 패스앱 호출\n",
       "        function appCall( platform, account ) {\n",
       "            if( platform ) {\n",
       "                var appLink = '';\n",
       "                var webUrl = '';\n",
       "\n",
       "                if( platform === 'youtube' ) {\n",
       "                    appLink = 'youtube://www.youtube.com/channel/UCEyXB3mjiTlZXJn_j1aKM7A';\n",
       "                    webUrl = 'https://www.youtube.com/channel/UCEyXB3mjiTlZXJn_j1aKM7A';\n",
       "                } else if( platform === 'instagram' ) {\n",
       "                    account = account ? account : 'monsterzym.official';\n",
       "\n",
       "                    appLink = 'instagram://user?username=' + account;\n",
       "                    webUrl = 'https://www.instagram.com/' + account;\n",
       "                }\n",
       "\n",
       "                if( appLink && webUrl ) {\n",
       "                    var ua = navigator.userAgent;\n",
       "                    var supportDevicesRegEx = /Android|iPhone|iPad/i;\n",
       "                    var supportDevice = ua.match(supportDevicesRegEx);\n",
       "                    var openAt = new Date;\n",
       "\n",
       "                    var launchApp = function (deviceName) {\n",
       "                        var src = appLink;\n",
       "\n",
       "                        var iframe = document.createElement('IFRAME')\n",
       "                        iframe.style.display = 'none';\n",
       "                        iframe.src = src;\n",
       "                        document.body.appendChild(iframe);\n",
       "\n",
       "                        failApp();\n",
       "                    };\n",
       "\n",
       "                    var isChromeAndBigThanChromeVersion18 = function () {\n",
       "                        var ua = navigator.userAgent;\n",
       "                        if (ua.indexOf('Chrome') == -1) {\n",
       "                            return false;\n",
       "                        }\n",
       "\n",
       "                        var versionRegEx = /Chrome\\/[0-9]+\\.[0-9]+\\.[0-9]+/;\n",
       "                        var majorVersion = ua.match(versionRegEx).pop().split('\\/')[2];\n",
       "                        return parseInt(majorVersion) > 18 ? true : false;\n",
       "                    };\n",
       "\n",
       "                    var isBigThanIOS9 = function () {\n",
       "                        var ua = navigator.userAgent;\n",
       "                        var versionRegEx = /iPhone OS [0-9]+/;\n",
       "                        var version = ua.match(versionRegEx).pop().split(' ')[2];\n",
       "                        var majorVersion = parseInt(version.split('_')[0]);\n",
       "                        return majorVersion >= 9 ? true : false;\n",
       "                    };\n",
       "\n",
       "                    var failApp = function () {\n",
       "                        var deLay = 500;\n",
       "                        setTimeout(function () {\n",
       "                            if (new Date - openAt < 2000) {\n",
       "                                var newWindow = window.open(webUrl);\n",
       "                            } else {\n",
       "                            }\n",
       "                        }, deLay);\n",
       "                    };\n",
       "\n",
       "                    if (!!supportDevice) {\n",
       "                        var deviceName = supportDevice.pop();\n",
       "                        launchApp(deviceName);\n",
       "                    } else {\n",
       "                        location.href = playStorePrefix;\n",
       "                    }\n",
       "                }\n",
       "            }\n",
       "        }\n",
       "\n",
       "        jQuery(document).ready(function() {\n",
